{
  "name": "pulsechecker_agent",
  "version": "0.0.1",
  "description": "A tool which allows to perform scheduled requests, pings, etc as well as receive events",
  "main": "index.js",
  "repository": "git@github.com:dpasechnik/pulsechecker_agent.git",
  "author": "Dmytro Pasichnyk <paskawmi@gmail.com>",
  "license": "BSD-2-Clause",
  "private": true,
  "dependencies": {
    "ajv": "^6.10.1",
    "bullmq": "^1.14.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "node-libcurl": "^2.2.0",
    "redis": "^2.8.0",
    "source-map-support": "^0.5.16",
    "tough-cookie": "^3.0.1",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "build": "babel src --out-dir dist --source-maps",
    "dev": "METRIC_SENDER_TYPE=stdout nodemon --watch ./src --ignore src/configsrv.js --exec babel-node src/index.js",
    "configsrv": "METRIC_SENDER_TYPE=stdout nodemon --watch ./src/configsrv.js --exec babel-node src/configsrv.js",
    "start": "node dist/index.js",
    "start-debug": "node --inspect=0.0.0.0:9229 dist/index.js",
    "test": "NODE_ENV=test METRIC_URL=/dev/null METRIC_SENDER_TYPE=blank jest --detectOpenHandles",
    "test:coverage": "NODE_ENV=test METRIC_SENDER_TYPE=blank jest --detectOpenHandles --coverage",
    "lint": "eslint --ext .js,.jsx ./src ./test",
    "lic": "license-checker"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@types/jest": "^25.1.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint_d": "^7.3.0",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.2",
    "license-checker": "^25.0.1",
    "mockdate": "^2.0.5",
    "nodemon": "^1.19.3",
    "supertest": "^4.0.2"
  }
}
